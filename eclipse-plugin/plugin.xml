<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.type"
            name="eFaps Data Model Type Content"
            priority="high">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="datamodel-type"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.sqltable"
            name="eFaps Data Model SQL Table Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="datamodel-sqltable"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.command"
            name="eFaps User Interface Command Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="ui-command"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.form"
            name="eFaps User Interface Form Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="ui-form"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.image"
            name="eFaps User Interface Image Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="ui-image"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.menu"
            name="eFaps User Interface Menu Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="ui-menu"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.search"
            name="eFaps User Interface Search Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="ui-search"/>
       </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="org.efaps.eclipse.contenttypes.table"
            name="eFaps User Interface Table Content">
       <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber">
         <parameter name="element" value="ui-table"/>
       </describer>
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            adaptable="true"
            icon="icons/datamodel/IMAGE_Admin_DataModel_TypeImage.gif"
            id="org.efaps.eclipse.decorators.type"
            label="eFaps Data Model Type Decorator"
            lightweight="true"
            location="TOP_LEFT"
            state="true">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IFile">
               </objectClass>
               <objectState
                     name="contentTypeId"
                     value="org.efaps.eclipse.contenttypes.type">
               </objectState>
            </and>
         </enablement>
      </decorator>
      <decorator
            adaptable="true"
            icon="icons/datamodel/IMAGE_Admin_DataModel_SQLTableImage.gif"
            id="org.efaps.eclipse.decorators.sqltable"
