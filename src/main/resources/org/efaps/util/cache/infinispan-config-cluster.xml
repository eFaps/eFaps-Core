        <!-- the Sql Query cache -->
        <local-cache name="${prefix:}org.efaps.db.QueryCache">
            <!--Only experation no eviction 10 minutes, 5 minutes (miliseconds) -->
            <expiration
                lifespan="600000"
                max-idle="300000" />
            <indexing
                storage="filesystem"
                path="${java.io.tmpdir}/infinispan"
                startup-mode="purge"
            >
                <indexed-entities>
                    <indexed-entity>org.efaps.db.QueryValue</indexed-entity>
                </indexed-entities>
            </indexing>
        </local-cache>
