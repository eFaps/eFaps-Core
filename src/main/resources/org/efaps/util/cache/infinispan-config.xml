    <namedCache name="Dimension4Name">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="UoM4ID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>

    <namedCache name="NumberGenerator4UUID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="NumberGenerator4ID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="NumberGenerator4Name">
        <eviction maxEntries="500" strategy="LIRS" />
