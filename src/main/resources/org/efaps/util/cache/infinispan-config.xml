    <namedCache name="AttributeType4Name">
        <eviction maxEntries="50" strategy="LRU" />
    </namedCache>

    <namedCache name="SQLTable4UUID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="SQLTable4ID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="SQLTable4Name">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>

    <namedCache name="AccessType4UUID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="AccessType4ID">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>
    <namedCache name="AccessType4Name">
        <eviction maxEntries="500" strategy="LIRS" />
    </namedCache>

    <namedCache name="Type4UUID">
        <eviction maxEntries="1500" strategy="LIRS" />
    </namedCache>
    <namedCache name="Type4ID">
        <eviction maxEntries="1500" strategy="LIRS" />
    </namedCache>
    <namedCache name="Type4Name">
        <eviction maxEntries="1500" strategy="LIRS" />
    </namedCache>

    <namedCache name="Attribute4Name">
        <eviction maxEntries="5000" strategy="LIRS" />
    </namedCache>
    <namedCache name="Attribute4ID">
        <eviction maxEntries="5000" strategy="LIRS" />
