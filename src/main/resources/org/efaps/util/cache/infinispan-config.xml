    <namedCache name="Group4Name">
        <eviction maxEntries="400" strategy="LIRS" />
    </namedCache>
    <namedCache name="Group4UUID">
        <eviction maxEntries="400" strategy="LIRS" />
    </namedCache>

    <namedCache name="Company4UUID">
        <eviction maxEntries="300" strategy="LIRS" />
    </namedCache>
    <namedCache name="Company4ID">
        <eviction maxEntries="300" strategy="LIRS" />
    </namedCache>
    <namedCache name="Company4Name">
        <eviction maxEntries="300" strategy="LIRS" />
    </namedCache>

    <namedCache name="Consortium4ID">
        <eviction maxEntries="200" strategy="LIRS" />
    </namedCache>
    <namedCache name="Consortium4Name">
        <eviction maxEntries="200" strategy="LIRS" />
    </namedCache>

    <namedCache name="Person4ID">
        <eviction maxEntries="100" strategy="LIRS" />
    </namedCache>
    <namedCache name="Person4Name">
        <eviction maxEntries="100" strategy="LIRS" />
    </namedCache>


    <namedCache name="JAASSystem4ID">
        <eviction maxEntries="5" strategy="LRU" />
    </namedCache>
    <namedCache name="JAASSystem4Name">
        <eviction maxEntries="5" strategy="LRU" />
    </namedCache>

    <namedCache name="Association4ID">
        <eviction maxEntries="50" strategy="LRU" />
    </namedCache>

    <!-- Other -->
    <namedCache name="Store4UUID">
        <eviction maxEntries="50" strategy="LRU" />
    </namedCache>
    <namedCache name="Store4ID">
        <eviction maxEntries="50" strategy="LRU" />
    </namedCache>
    <namedCache name="Store4Name">
