<common-systemattribute>
  <uuid>85d94368-bc1e-49bf-88d7-a3912b50e938</uuid>
  <description>This Attribute defines the maximum number of tries to login with
    the wrong Password into eFaps, before the User is going to be deactivated. 
    To deactivate this mechanisem set the value to 0.
  </description>
  <definition>
    <version>
      <application>eFaps</application>
      <global>1</global>
      <local>$Rev$</local>
      <mode>true</mode>
      <mode>(version==9)</mode>
    </version>
    <name>Admin_User_LoginTries</name>
    <value>3</value>    
  </definition>
</common-systemattribute>
