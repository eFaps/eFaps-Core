<xsd:schema targetNamespace="http://www.efaps.org/xsd"
            elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:eFaps="http://www.efaps.org/xsd">

  <!-- RootElements -->
  <xsd:element name="access-set">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_access-set" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="access-type">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_access-type" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="common-systemconfiguration">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_common-systemconfig" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="datamodel-sqltable">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_datamodel-sqltable" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="datamodel-type">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_datamodel-type" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="dbproperties">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="bundle" type="eFaps:bundle" maxOccurs="1" minOccurs="1"></xsd:element>
            <xsd:element name="resource" type="eFaps:resource" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="import">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="object" type="eFaps:import_object" maxOccurs="unbounded" minOccurs="1"></xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="install">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="application" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element name="version" type="eFaps:install_version" maxOccurs="unbounded" minOccurs="1"></xsd:element>
        <xsd:element name="files" type="eFaps:install_files" maxOccurs="unbounded" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>This elementsare created automatically by the maven goal;</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="integration-webdav">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_integration-webdav" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ui-command">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_ui-command" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ui-form">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_ui-form" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ui-image">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_ui-image" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ui-menu">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_ui-menu" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ui-search">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_ui-search" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ui-table">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_ui-table" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="user-jaassystem">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_user-jaassystem" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="user-role">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="eFaps:AbstractBase">
          <xsd:sequence>
            <xsd:element name="definition" type="eFaps:definition_user-role" maxOccurs="unbounded" minOccurs="1"></xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>



  <!-- Abstract Types -->
  <xsd:complexType name="AbstractBase">
    <xsd:sequence>
      <xsd:element name="uuid" type="xsd:string" maxOccurs="1" minOccurs="1" nillable="false"/>
      <xsd:element name="file-application" type="xsd:string" maxOccurs="1" minOccurs="0"/>
      <xsd:element name="file-revision" type="xsd:string" maxOccurs="1" minOccurs="0"/>
      <xsd:element name="description" type="xsd:string" maxOccurs="1" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AbstractDefinition">
    <xsd:sequence>
      <xsd:element name="version-expression" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
      <xsd:element name="parent" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>


  <!-- Types -->
  <xsd:complexType name="access">
    <xsd:sequence>
      <xsd:element name="group" type="xsd:string" maxOccurs="unbounded" minOccurs="0"></xsd:element>
      <xsd:element name="person" type="xsd:string" maxOccurs="unbounded" minOccurs="0"></xsd:element>
      <xsd:element name="role" type="xsd:string" maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>


  <xsd:complexType name="assigned-role-person">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="key" type="xsd:string"></xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>


  <xsd:complexType name="attribute">
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="type" maxOccurs="1" minOccurs="1">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Boolean" />
            <xsd:enumeration value="Created" />
            <xsd:enumeration value="CreatorLink" />
            <xsd:enumeration value="Date"/>
            <xsd:enumeration value="DateTime"/>
            <xsd:enumeration value="Decimal"/>
            <xsd:enumeration value="Integer" />
            <xsd:enumeration value="Link" />
            <xsd:enumeration value="LinkWithRanges"/>
            <xsd:enumeration value="Long" />
            <xsd:enumeration value="Modified" />
            <xsd:enumeration value="ModifierLink" />
            <xsd:enumeration value="OID" />
            <xsd:enumeration value="Password"/>
            <xsd:enumeration value="PersonLink"/>
            <xsd:enumeration value="PolicyLink"/>
            <xsd:enumeration value="Real"></xsd:enumeration>
            <xsd:enumeration value="String"/>
            <xsd:enumeration value="Time"/>
            <xsd:enumeration value="Type" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="typelink" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
      <xsd:element name="sqltable" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="sqlcolumn" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="defaultvalue" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
      <xsd:element name="validate" type="eFaps:validate" maxOccurs="unbounded" minOccurs="0"></xsd:element>
      <xsd:element name="trigger" type="eFaps:trigger4Attribute" maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="attributeset">
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="type" maxOccurs="1" minOccurs="1">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MultiLineArray" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="parent" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
      <xsd:element name="sqltable" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="sqlcolumn" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="attribute" type="eFaps:attribute" maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>
 
  <xsd:complexType name="bundle">
    <xsd:attribute name="name" type="xsd:string"></xsd:attribute>
    <xsd:attribute name="sequence" type="xsd:string"></xsd:attribute>
  </xsd:complexType>


  <xsd:complexType name="childs">
    <xsd:sequence>
      <xsd:element name="child" type="xsd:string" maxOccurs="unbounded" minOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="column">
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="type" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="BOOLEAN"/>
          <xsd:enumeration value="BLOB"/>
          <xsd:enumeration value="CLOB"/>
          <xsd:enumeration value="DATETIME"/>
          <xsd:enumeration value="DECIMAL"/>
          <xsd:enumeration value="INTEGER"/>
          <xsd:enumeration value="REAL"/>
          <xsd:enumeration value="STRING_LONG"/>
          <xsd:enumeration value="STRING_SHORT"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="length" type="xsd:integer" use="optional"/>
    <xsd:attribute name="not-null" type="xsd:boolean" use="optional"/>
    <xsd:attribute name="scale" type="xsd:integer" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="configAttribute">
    <xsd:sequence>
      <xsd:element name="key" type="xsd:string" maxOccurs="1"/>
      <xsd:element name="value" type="xsd:string" maxOccurs="1"/>
      <xsd:element name="description" type="xsd:string" maxOccurs="1" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>


  <xsd:complexType name="database">
    <xsd:sequence>
      <xsd:element name="table-name" type="xsd:string" maxOccurs="1"></xsd:element>
      <xsd:element name="parent-table" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
      <xsd:element name="column" type="eFaps:column" minOccurs="0" maxOccurs="unbounded"></xsd:element>
      <xsd:element name="unique" type="eFaps:unique" minOccurs="0" maxOccurs="unbounded"></xsd:element>
      <xsd:element name="foreign" type="eFaps:foreign" minOccurs="0" maxOccurs="unbounded"></xsd:element>
      <xsd:element name="sql" type="xsd:string" maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>


  <xsd:complexType name="default">
    <xsd:sequence>
      <xsd:element name="command" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  
  <xsd:complexType name="definition_access-set">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="access-type" type="xsd:string" maxOccurs="unbounded"></xsd:element>
          <xsd:element name="type" type="xsd:string" maxOccurs="unbounded"></xsd:element>
          <xsd:element name="role" type="xsd:string" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_access-type">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition"></xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_common-systemconfig">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="attribute" type="eFaps:configAttribute" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_datamodel-sqltable">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="typeid-column" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="database" type="eFaps:database" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_datamodel-type">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="abstract" type="xsd:boolean" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:group ref="eFaps:attributeGroup" maxOccurs="unbounded" minOccurs="0"></xsd:group>
          <xsd:element name="trigger" type="eFaps:trigger4Type" maxOccurs="unbounded" minOccurs="0"></xsd:element>
          <xsd:element name="property" type="eFaps:property_datamodel-type" maxOccurs="unbounded" minOccurs="0"></xsd:element>
          <xsd:element name="event-for" type="eFaps:event-for" minOccurs="0" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_integration-webdav">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="path" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="property" type="eFaps:property" maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_ui-command">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="icon" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="access" type="eFaps:access" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="target" type="eFaps:target" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="trigger" type="eFaps:trigger4UI" maxOccurs="unbounded" minOccurs="0"></xsd:element>
          <xsd:element name="property" type="eFaps:property_ui-command" maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_ui-form">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="field" type="eFaps:field" maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_ui-image">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="file" type="xsd:string" maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="type" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_ui-menu">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="icon" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="access" type="eFaps:access" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="target" type="eFaps:target" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="type" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="childs" type="eFaps:childs" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="trigger" type="eFaps:trigger4UI" maxOccurs="unbounded" minOccurs="0"></xsd:element>
          <xsd:element name="property" type="eFaps:property_ui-command" maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_ui-search">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="icon" type="xsd:string" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="default" type="eFaps:default" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="childs" type="eFaps:childs" maxOccurs="1" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_ui-table">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="field" type="eFaps:field" maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_user-jaassystem">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="person" type="eFaps:person" maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="role" type="eFaps:jaassystem_role-group" maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="group" type="eFaps:jaassystem_role-group" maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="assigned-person" type="eFaps:assigned-role-person" maxOccurs="unbounded" minOccurs="0"></xsd:element>
          <xsd:element name="assigned-role" type="eFaps:assigned-role-person" maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="definition_user-role">
    <xsd:complexContent>
      <xsd:extension base="eFaps:AbstractDefinition">
        <xsd:sequence>
          <xsd:element name="status" type="xsd:boolean" maxOccurs="1" minOccurs="1" default="false"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="evaluate">
    <xsd:sequence>
      <xsd:element name="property" type="eFaps:property" maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
    <xsd:attribute name="program" type="xsd:string" />
  </xsd:complexType>


  <xsd:complexType name="event-for">
    <xsd:attribute name="type" type="xsd:string" />
  </xsd:complexType>

  <xsd:complexType name="execute">
    <xsd:sequence>
      <xsd:element name="property" type="eFaps:property" maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
    <xsd:attribute name="program" type="xsd:string" />
    <xsd:attribute name="method" type="xsd:string" use="optional" default="execute" />
  </xsd:complexType>


  <xsd:complexType name="field">
    <xsd:sequence>
      <xsd:element name="icon" type="xsd:string" maxOccurs="1" minOccurs="0"/>
      <xsd:element name="property" type="eFaps:property_field" maxOccurs="unbounded" minOccurs="0"/>
      <xsd:element name="trigger" type="eFaps:trigger4UI" maxOccurs="unbounded" minOccurs="0"/>
      <xsd:element name="table" type="xsd:string" maxOccurs="1" minOccurs="0"/>
      <xsd:element name="evaluate" type="eFaps:evaluate" maxOccurs="1" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="name" type="xsd:string" use="required"></xsd:attribute>
    <xsd:attribute name="character" use="optional" default="Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Command"/>
          <xsd:enumeration value="Expression" />
          <xsd:enumeration value="Group" />
          <xsd:enumeration value="Heading" />
          <xsd:enumeration value="Set"/>
          <xsd:enumeration value="Target" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>


  <xsd:complexType name="foreign">
    <xsd:attribute name="name" type="xsd:string"/>
    <xsd:attribute name="key" type="xsd:string"/>
    <xsd:attribute name="reference" type="xsd:string"/>
